{
  "name": "noter-frontend",
  "version": "1.0.0",
  "description": "A React Boilerplate Configuration",
  "main": "index.js",
  "scripts": {
    "build": "webpack-cli --mode=production --env=prod",
    "start": "webpack-dev-server --mode=development --env=dev",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "author": "Boy With Silver Wings <agney@outlook.in>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^22.4.4",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^1.1.12",
    "copy-webpack-plugin": "^4.5.4",
    "css-loader": "^6.10.0",
    "cssnano": "^4.1.7",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.10.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^1.1.2",
    "jest": "^29.3.1",
    "jest-enzyme": "^4.2.0",
    "npm-install-webpack-plugin": "^4.0.5",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.2.0",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0",
    "purify-css": "^1.2.6",
    "purifycss-webpack": "^0.7.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.6.0",
    "style-loader": "^0.18.2",
    "stylelint": "^15.10.1",
    "stylelint-config-standard": "^17.0.0",
    "tapable": "^1.1.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^0.5.9",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^4.1.4",
    "webpack-monitor": "^1.0.14",
    "workbox-webpack-plugin": "^2.1.3"
  },
  "dependencies": {
    "antd": "^3.10.3",
    "axios": "^1.6.0",
    "date-fns": "^1.29.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-infinite-scroller": "^1.2.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.0.2",
    "whatwg-fetch": "^2.0.4"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/scripts/test.js",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/scripts/**"
    ],
    "testPathIgnorePatterns": [
      "/node_modules",
      "/scripts",
      "/build-utils"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BoyWithSilverWings/react-app.git"
  }
}
